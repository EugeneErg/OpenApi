{
  "name": "eugene-erg/open-api",
  "type": "project",
  "description": "OpenApi Builder",
  "license": "MIT",
  "require": {
    "php": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "EugeneErg\\OpenApi\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.58",
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^10.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
